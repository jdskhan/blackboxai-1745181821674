{
  "name": "usdt-tds-management-electron",
  "version": "1.0.0",
  "description": "USDT Trading & TDS Management System Electron App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.example.usdttds",
    "mac": {
      "category": "public.app-category.finance",
      "target": "dmg"
    }
  },
  "dependencies": {
    "electron": "^25.3.1"
  },
  "devDependencies": {
    "electron-builder": "^23.6.0"
  }
}
